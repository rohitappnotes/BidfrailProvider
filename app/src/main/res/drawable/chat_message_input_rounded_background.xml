<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />

            <stroke
                android:width="0.5dp"
                android:color="#52535353" />

            <corners
                android:bottomLeftRadius="@dimen/_100sdp"
                android:bottomRightRadius="@dimen/_100sdp"
                android:topLeftRadius="@dimen/_100sdp"
                android:topRightRadius="@dimen/_100sdp" />

            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp" />
        </shape>
    </item>
</layer-list>